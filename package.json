    "@grammyjs/parse-mode": "^1.7.1",
    "@grammyjs/ratelimiter": "^1.2.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/storage-free": "^2.3.1",
    "@grammyjs/storage-mongodb": "^2.3.1",
    "@grammyjs/storage-prisma": "^2.3.1",
    "@grammyjs/transformer-throttler": "^1.2.1",
    "@prisma/client": "^5.2.0",
    "archiver": "^5.3.2",
    "axios": "^1.5.0",
    "base-x": "^4.0.0",
    "callback-data": "^1.1.0",
    "commander": "^11.0.0",
