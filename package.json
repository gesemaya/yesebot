    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "expr-eval": "^2.0.2",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^118.0.0",
    "gpt-3-encoder": "^1.1.4",
    "grammy": "^1.18.1",
    "grammy-inline-menu": "^8.0.1",
    "grammy-middlewares": "^1.0.11",
    "jimp": "^0.22.10",
    "markup": "link:@grammyjs/types/markup",
    "mongoose": "^7.5.0",
    "node-cache": "^5.1.2",
    "node-emoji": "^1.11.0",
    "nodemon": "^2.0.22",
    "openai": "^3.3.0",
